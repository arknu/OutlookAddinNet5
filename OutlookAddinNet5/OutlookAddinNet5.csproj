    </None>
    <None Update="Resources\Command2.png">
      <CustomToolNamespace>OutlookAddinNet5.Resources</CustomToolNamespace>
    </None>
    <None Update="Resources\Ribbon.xml">
      <CustomToolNamespace>OutlookAddinNet5.Resources</CustomToolNamespace>
    </None>
  </ItemGroup>

</Project>
